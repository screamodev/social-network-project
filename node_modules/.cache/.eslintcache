xStore\";\nimport { withSuspense } from \"./hoc/withSuspense\";\n\nconst DialogsContainer = React.lazy(() =>\n  import(\"./components/Dialogs/DialogsContainer\")\n);\nconst ProfileContainer = React.lazy(() =>\n  import(\"./components/Profile/ProfileContainer\")\n);\n\n// rafce\n\n// rfcredux//connect\n\nclass App extends Component {\n  componentDidMount() {\n    this.props.initializeApp();\n  }\n  render() {\n    if (!this.props.initialized) {\n      return <PreLoader />;\n    } else\n    return (\n      <div className=\"app-wrapper\">\n        <HeaderContainer />\n        <Navbar />\n        <div className=\"app-wrapper-content\">\n          <Route\n            path=\"/profile/:userId?\"\n            render={withSuspense(ProfileContainer)}\n          />\n          <Route path=\"/dialogs\" render={withSuspense(DialogsContainer)} />\n          <Route path=\"/users\" render={() => <UsersContainer />} />\n          <Route path=\"/login\" render={() => <Login />} />\n          <Route path=\"/gallery\" component={Gallery} />\n          <Route path=\"/news\" component={News} />\n          <Route path=\"/music\" component={Music} />\n          <Route path=\"/settings\" component={Settings} />\n        </div>\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = (state) => ({\n  initialized: state.app.initialized,\n});\n\nlet AppContainer = compose(\n  withRouter,\n  connect(mapStateToProps, { initializeApp })\n)(App);\n\nlet SocialNetworkApp = (props) => {\n  return (\n    <HashRouter>\n      <Provider store={store}>\n        <AppContainer />\n      </Provider>\n    </HashRouter>\n  );\n};\n\nexport default SocialNetworkApp;\n","/home/screamoviolence/myDream/js/react-practise/my-first-app/src/reportWebVitals.js",[],"/home/screamoviolence/myDream/js/react-practise/my-first-app/src/components/Profile/Profile.jsx",[],"/home/screamoviolence/myDream/js/react-practise/my-first-app/src/components/Header/Header.jsx",[],"/home/screamoviolence/myDream/js/react-practise/my-first-a